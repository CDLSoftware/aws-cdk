  "description": "CDK Constructs for AWS Glue",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "bundledDependencies": [],
    "names": {
      "java": "com.amazonaws.cdk.glue",
      "dotnet": "Aws.Cdk.Glue"
    }
  },
  "repository": {
