  },
  "dependencies": {
    "aws-cdk": "^0.6.0",
    "aws-cdk-s3": "^0.6.0",
    "aws-cdk-iam": "^0.6.0",
    "aws-cdk-kms": "^0.6.0",
    "aws-cdk-resources": "^0.6.0"
